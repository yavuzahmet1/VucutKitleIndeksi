import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int kg;
        double boy;

        System.out.print("Lütfen boyunuzu (metre cinsinde) giriniz\t: ");
        boy = scanner.nextDouble();
        System.out.println("Lütfen kilonuzu giriniz\t :");
        kg=scanner.nextInt();
        System.out.println("Vücut Kitle İndeksiniz\t :"+kg/(boy*boy));
    }
}
